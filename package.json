{
  "name": "kiku",
  "private": true,
  "main": "electron/main.ts",
  "version": "1.0.0",
  "description": "Kiku",
  "repository": "",
  "license": "UNLICENSED",
  "homepage": "./",
  "scripts": {
    "start": "electron-forge start",
    "dev": "cross-env NODE_ENV=development vite --port=5201",
    "dev2": "vite dev --port=5201",
    "build": "yarn lint && cross-env NODE_ENV=production vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
    "electron-start": "concurrently --kill-others \"yarn dev\" \"wait-on http://localhost:5201/index.html && cross-env NODE_ENV=development electron .\"",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.14",
    "axios": "^1.5.1",
    "dom7": "^4.0.6",
    "electron-squirrel-startup": "^1.0.0",
    "framework7": "^8.3.0",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.3.0",
    "material-icons": "^1.13.12",
    "nanoid": "^5.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "skeleton-elements": "^4.0.1",
    "swiper": "^10.3.1",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "wavesurfer.js": "^7.4.2",
    "youtubei.js": "^7.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "electron": "^27.0.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.31",
    "postcss-preset-env": "^9.2.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "typescript-eslint-parser": "^22.0.0",
    "vite": "^4.5.0",
    "wait-on": "^7.0.1"
  }
}
